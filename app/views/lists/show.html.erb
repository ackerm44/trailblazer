<h1><%= @list.name %></h1>
<h4><%= @list.description %></h4>
<ul>
  <% @list.trails.each do |trail| %>
    <li><%= link_to trail.name, trail_path(trail) %></li>
  <% end %>
</ul>
<br>
<%= link_to "Edit List", edit_list_path(@list) %><br>
<%= link_to "Delete List", list_path(@list), :method => 'delete' %>
